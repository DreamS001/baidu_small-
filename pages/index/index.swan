        <text>证件识别</text>
    </navigator>
    <navigator url="../car/car" class="grild_item">
        <image src="../../images/car.png" />
        <text>车辆识别</text>
    </navigator>
    <navigator url="../bank/bank" class="grild_item">
        <image src="../../images/bank_car.png" />
