<view class='userinfo'>
    <image src='{{userInfo.avatarUrl}}' class='user_img' />
    <view>{{userInfo.nickName}}</view>
    <block s-if="{{userInfo.gender==0}}">
        <view>女</view>
    </block>
    <block s-else>
        <view>男</view>
    </block>
</view>
